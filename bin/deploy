#! /bin/bash

ssh -t ent@maven.itap.purdue.edu 'mv www/* backups/$(date +'%-d-%-m-%-Y-%-H-%-M-%-S')'
echo
echo "** BACKUP CREATED **"
echo
scp -r * ent@maven.itap.purdue.edu:~/www/
echo
echo "** SCP COMPLETE **"
echo
ssh -t ent@maven.itap.purdue.edu 'cd www/; chmod -R 755 *; echo "** CHMOD COMPLETE **";'
